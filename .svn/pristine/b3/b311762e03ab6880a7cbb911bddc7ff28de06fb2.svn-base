package fr.insalyon.dasi.proactif.metier.OM;

import java.io.Serializable;
//import java.text.SimpleDateFormat;
import java.util.Date;
import javax.persistence.Entity;
import javax.persistence.OneToOne;
import javax.persistence.Temporal;

/**
 *
 * @author mduraffour
 */
@Entity
public class Employe extends Personne implements Serializable {
    
    //@Id
    //@GeneratedValue(strategy = GenerationType.IDENTITY)
    //protected Integer IdEmp;
    @Temporal(javax.persistence.TemporalType.DATE)
    protected Date HeureDebut;
    @Temporal(javax.persistence.TemporalType.DATE)
    protected Date HeureFin;
    protected Boolean Disponibilite;
    @OneToOne
    protected Intervention InterventionActive;

    //Constructeur
    public Employe(Date HeureDebut, Date HeureFin, Boolean Disponibilite, 
            Intervention InterventionActive, String civilite, String nom, 
            String prenom, Date dateNaiss, String adressePost, String numTel, 
            String mail, String mdp) {
        super(civilite, nom, prenom, dateNaiss, adressePost, numTel, mail, mdp);
        this.HeureDebut = HeureDebut;
        this.HeureFin = HeureFin;
        this.Disponibilite = Disponibilite;
        this.InterventionActive = InterventionActive;
    }
    
    public Employe() {
        super();
    }
    
    
    //getter
    public Date getHeureDebut() {
        return HeureDebut;
    }

    public Date getHeureFin() {
        return HeureFin;
    }

    public Boolean getDisponibilite() {
        return Disponibilite;
    }
    
    
}
